{
  "name": "hausarbeit-webentw",
  "version": "1.0.0",
  "description": "Hausarbeit Webentwicklung",
  "main": "./js/Main.js",
  "dependencies": {
    "express": ">= 4.15.3",
    "request": ">= 2.81.0",
    "google-maps":">= 3.2.1",
    "fs": ">= 0.0.1",
    "path": ">= 0.12.7",
    "delete": ">= 1.0.1"
  },
  "devDependencies": {
    "browserify": ">= 14.4.0",
    "eslint": ">= 4.0.0",
    "less": ">= 2.7.2",
    "babili": ">= 0.1.3",
    "remove-node-modules": ">= 1.7.2",
    "del-cli": ">= 1.0.0"
  },
  "scripts": {
    "lint":"eslint ./js/*.js",
    "lintserver": "eslint ../../server/*.js",
    "js":"browserify ./js/Main.js > ./main.js",
    "jsdebug": "browserify ./js/Main.js > ./release/main.js",
    "less":"lessc ./styles/styles.less ./styles.css",
    "lessdebug": "lessc ./styles/styles.less ./release/styles.css",
    "minijs":"babili ./main.js > ./release/main.js",
    "minicss":"lessc --clean-css ./styles/styles.less ./release/styles.css",
    "build":"npm run lint && npm run lintserver && npm run js && npm run minijs && npm run minicss",
    "start":"node ../../server/MainServer.js",
    "debug":"npm run lint && npm run lintserver && npm run jsdebug && npm run lessdebug",
    "debug_start":"npm run debug && npm run start",
    "build_start":"npm run build && npm run start",
    "clean":"del-cli ./release/main.js && del-cli ./release/styles.css && del-cli ./main.js && del-cli ./styles.css && del-cli ./styles/styles.css && del-nodem",
    "del-nodem": " remove-node-modules ./node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Kevin KÃ¼chler, Stefan Fuhrmann",
  "license": "ISC"
}
